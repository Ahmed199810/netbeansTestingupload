<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="508.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="arduinoosc.FXMLDocumentController">
    <children>
        <Button fx:id="btn_connect" layoutX="229.0" layoutY="14.0" onAction="#handleButtonAction" text="Connect" />
      <ComboBox fx:id="combo" layoutX="14.0" layoutY="14.0" prefWidth="150.0" />
      <Button fx:id="btn_on_off" layoutX="314.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnONOFF" text="ON" />
      <LineChart fx:id="line_char" layoutX="14.0" layoutY="94.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="638.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnStartPlot" text="Start Plot" />
    </children>
</AnchorPane>
